{
    "name": "niyam/bpms",
    "description": "A simple BPMS Library.",
    "keywords": ["bpms", "niyam", "workflow"],
    "license": "MIT",
    "authors": [{
        "name": "David Dashti",
        "email": "mddashti@gmail.com"
    }],
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^6.*",
        "illuminate/support": "^6.*",
        "nesbot/carbon": "^2.19",
        "symfony/dom-crawler": "^3.3",
        "symfony/expression-language": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.2.*"
    },
    "autoload": {
        "psr-4": {
            "Niyam\\Bpms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Niyam\\Bpms\\BpmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
